#!/usr/bin/env node
// Read environment variable from registry
const { registry } = require('../dist/registry/index.js');

const args = process.argv.slice(2);
const varName = args[0];
const defaultValue = args[1] || '';

if (!varName) {
  console.error('Usage: _read_env <var_name> [default_value]');
  process.exit(1);
}

const value = registry.get(varName, defaultValue);
console.log(value);
