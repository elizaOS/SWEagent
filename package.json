{
  "name": "swe-agent-ts",
  "version": "1.0.0",
  "description": "swe-agent-ts: Software Engineering Agent built in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "sweagent": "./dist/run/cli.js"
  },
  "files": [
    "dist/",
    "tools/bin/",
    "tools/dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc && cd tools && npm run build",
    "test": "jest && bash test-all-tools.sh",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "clean": "rm -rf dist coverage",
    "dev": "ts-node src/index.ts",
    "rules": "ts-node src/rules/cli.ts",
    "rules:validate": "ts-node src/rules/cli.ts validate",
    "rules:export": "ts-node src/rules/cli.ts export",
    "rules:info": "ts-node src/rules/cli.ts info"
  },
  "keywords": [
    "software-engineering",
    "agent",
    "ai",
    "automation",
    "development-tools"
  ],
  "author": "ElizaOS",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "axios": "^1.11.0",
    "chalk": "^5.6.0",
    "commander": "^11.1.0",
    "diff": "^5.2.0",
    "dockerode": "^4.0.7",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "glob": "^10.4.5",
    "inquirer": "^9.3.7",
    "js-yaml": "^4.1.0",
    "jsonl": "^1.1.2",
    "minimatch": "^9.0.5",
    "ora": "^7.0.1",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.55.0",
    "sharp": "^0.33.5",
    "simple-git": "^3.28.0",
    "strip-ansi": "^7.1.0",
    "tar-stream": "^3.1.7",
    "wrap-ansi": "^8.1.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/diff": "^5.2.3",
    "@types/dockerode": "^3.3.43",
    "@types/express": "^4.17.23",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.11",
    "@types/tar-stream": "^3.1.4",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/swe-agent-ts"
  },
  "bugs": {
    "url": "https://github.com/elizaos/swe-agent-ts/issues"
  },
  "homepage": "https://github.com/elizaos/swe-agent-ts#readme"
}
